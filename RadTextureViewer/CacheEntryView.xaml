<reactiveui:ReactiveUserControl x:Class="RadTextureViewer.CacheEntryView"
        x:TypeArguments="rad:CacheEntry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:reactiveui="http://reactiveui.net"
        xmlns:rad="clr-namespace:RadTextureViewer.Core;assembly=RadTextureViewer.Core"
        xmlns:local="clr-namespace:RadTextureViewer"
        mc:Ignorable="d"
        d:DesignHeight="450" d:DesignWidth="800">
    <Control.ToolTip>
        <TextBlock><Run x:Name="uuid"/><LineBreak/><Run x:Name="time"/><LineBreak/><Run x:Name="size"/> KB</TextBlock>
    </Control.ToolTip>
    <Image x:Name="image" HorizontalAlignment="Center" VerticalAlignment="Center" Width="64" Height="64"/>
</reactiveui:ReactiveUserControl>
